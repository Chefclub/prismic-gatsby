{
  "name": "gatsby-plugin-prismic-previews-2",
  "private": true,
  "version": "4.0.0",
  "description": "Gatsby plugin for integrating client-side Prismic previews support",
  "license": "MIT",
  "main": "./dist/gatsby-source-prismic-previews.js",
  "module": "./dist/gatsby-source-prismic-previews.module.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle build src/index.ts src/gatsby-node.ts src/gatsby-browser.ts --tsconfig tsconfig.build.json --jsx React.createElement",
    "dev": "microbundle watch src/index.ts src/gatsby-node.ts src/gatsby-browser.ts --tsconfig tsconfig.build.json --jsx React.createElement",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismic"
  ],
  "dependencies": {
    "camel-case": "^4.1.2",
    "chalk": "^4.1.0",
    "es-cookie": "^1.3.2",
    "fp-ts": "^2.9.3",
    "gatsby-node-helpers": "^1.0.3",
    "prismic-javascript": "^3.0.2",
    "tiny-hashes": "^1.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.29",
    "gatsby-source-prismic": ">=4",
    "react": ">=16.13"
  },
  "devDependencies": {
    "type-fest": "^0.20.2"
  }
}
