{
  "name": "gatsby-plugin-prismic-previews",
  "private": true,
  "version": "3.2.1",
  "description": "Gatsby plugin for integrating Prismic previews support",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismic"
  ],
  "dependencies": {
    "camel-case": "^4.1.2",
    "es-cookie": "^1.3.2",
    "fp-ts": "^2.9.3",
    "gatsby-node-helpers": "^1.0.3",
    "tiny-hashes": "^1.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.29",
    "gatsby-source-prismic": ">=4",
    "react": ">=16.13"
  }
}
