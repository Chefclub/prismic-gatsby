{
  "name": "gatsby-plugin-prismic-previews",
  "private": true,
  "version": "4.0.0",
  "description": "Gatsby plugin for integrating client-side Prismic previews support",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "gatsby-browser.js",
    "gatsby-ssr.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production concurrently -p '[{name}]' -n 'esbuild,    tsc' -c 'yellow.dim,blue.dim' 'node ./build.mjs' 'tsc -p ./tsconfig.build.json'",
    "dev": "NODE_ENV=development concurrently -p '[{name}]' -n 'esbuild,    tsc' -c 'yellow.dim,blue.dim' 'node ./build.mjs -w' 'tsc -w --preserveWatchOutput -p ./tsconfig.build.json'",
    "gen": "tcm src",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "ava test/**/*.ava.test.{ts,tsx}",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismic"
  ],
  "dependencies": {
    "@reach/visually-hidden": "^0.13.2",
    "camel-case": "^4.1.2",
    "chalk": "^4.1.0",
    "es-cookie": "^1.3.2",
    "fp-ts": "^2.9.5",
    "gatsby-node-helpers": "^1.2.1",
    "gatsby-plugin-imgix": "^0.9.0",
    "ky": "^0.27.0",
    "ky-universal": "^0.9.1",
    "prismic-dom": "^2.2.5",
    "tiny-hashes": "^1.0.1",
    "ts-prismic": "^0.3.0"
  },
  "peerDependencies": {
    "gatsby": ">=2.29",
    "gatsby-source-prismic": ">=4",
    "react": ">=16.13"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.0.3",
    "@types/jsdom": "^16.2.7",
    "browser-env": "^3.3.0",
    "cross-fetch": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^16.5.1",
    "react-test-renderer": "^17.0.1",
    "source-map-explorer": "^2.5.2",
    "type-fest": "^0.21.1",
    "typed-css-modules": "^0.6.5"
  },
  "ava": {
    "extensions": [
      "ts",
      "tsx"
    ],
    "require": [
      "ts-eager/register"
    ],
    "environmentVariables": {
      "TS_NODE_IGNORE": "(?:^|/)node_modules(?!/ky)/"
    },
    "verbose": true
  }
}
