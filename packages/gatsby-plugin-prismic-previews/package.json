{
  "name": "gatsby-plugin-prismic-previews",
  "private": true,
  "version": "4.0.0",
  "description": "Gatsby plugin for integrating client-side Prismic previews support",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "gatsby-browser.js",
    "gatsby-ssr.js"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production concurrently -p '[{time}-{name}]' -n 'esbuild,tsc' -c 'yellow,blue' 'node ./build.mjs' 'tsc -p ./tsconfig.build.json'",
    "dev": "NODE_ENV=development concurrently -p '[{time}-{name}]' -n 'esbuild,tsc' -c 'yellow,blue' 'node ./build.mjs -w' 'tsc -w --preserveWatchOutput -p ./tsconfig.build.json'",
    "gen": "tcm src",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismic"
  ],
  "dependencies": {
    "@reach/visually-hidden": "^0.13.0",
    "camel-case": "^4.1.2",
    "chalk": "^4.1.0",
    "es-cookie": "^1.3.2",
    "esbuild": "^0.8.45",
    "fp-ts": "^2.9.5",
    "gatsby-node-helpers": "^1.0.3",
    "ky": "^0.26.0",
    "prismic-dom": "^2.2.4",
    "tiny-hashes": "^1.0.1",
    "ts-prismic": "^0.2.2"
  },
  "peerDependencies": {
    "gatsby": ">=2.29",
    "gatsby-source-prismic": ">=4",
    "react": ">=16.13"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.0.3",
    "cross-fetch": "^3.0.6",
    "gatsby-plugin-utils": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "react-test-renderer": "^17.0.1",
    "source-map-explorer": "^2.5.2",
    "type-fest": "^0.21.0",
    "typed-css-modules": "^0.6.5"
  }
}
