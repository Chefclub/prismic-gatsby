{
  "name": "gatsby-plugin-prismic-previews",
  "version": "4.1.2",
  "description": "Gatsby plugin for integrating client-side Prismic previews support",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "prismic",
    "preview"
  ],
  "homepage": "https://github.com/prismicio/prismic-gatsby/tree/main/packages/gatsby-plugin-prismic-previews",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/prismicio/prismic-gatsby.git"
  },
  "license": "Apache-2.0",
  "author": "Prismic <contact@prismic.io> (https://prismic.io)",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./gatsby-node": {
      "require": "./dist/gatsby-node.cjs"
    },
    "./gatsby-browser": {
      "require": "./dist/gatsby-browser.cjs",
      "import": "./dist/gatsby-browser.mjs"
    },
    "./gatsby-ssr": {
      "require": "./dist/gatsby-ssr.cjs",
      "import": "./dist/gatsby-ssr.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "gatsby-browser.js",
    "gatsby-ssr.js"
  ],
  "scripts": {
    "build": "siroc build",
    "dev": "siroc build --watch",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "lint": "eslint --ext .js,.ts .",
    "unit": "nyc --reporter=lcovonly --reporter=text --exclude-after-remap=false ava",
    "test": "npm run lint && npm run unit",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook --quiet -p 6006"
  },
  "dependencies": {
    "@imgix/gatsby": "^1.6.11",
    "@prismicio/client": "^6.0.0-alpha.11",
    "@prismicio/helpers": "^2.0.0-alpha.4",
    "@prismicio/mock": "^0.0.5",
    "@prismicio/types": "^0.1.6",
    "@reach/dialog": "^0.16.0",
    "camel-case": "^4.1.2",
    "clsx": "^1.1.1",
    "es-cookie": "^1.3.2",
    "fp-ts": "^2.10.5",
    "gatsby-node-helpers": "^1.2.1",
    "react-shadow": "^19.0.2",
    "tiny-hashes": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/cli": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@themosaad/tailwindcss-capsize": "^1.0.0",
    "abort-controller": "^3.0.0",
    "browser-env": "^3.3.0",
    "cross-fetch": "^3.1.4",
    "gatsby-plugin-image": "^1.11.0",
    "global-jsdom": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^16.7.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-polymorphic-types": "^2.0.0",
    "react-test-renderer": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "siroc": "^0.16.0",
    "source-map-explorer": "^2.5.2",
    "to-string-loader": "^1.1.6"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0",
    "gatsby-plugin-image": "^1.3.0-next.1",
    "gatsby-source-prismic": "^4.0.0-alpha.0",
    "react": ">=16.13"
  },
  "engines": {
    "node": ">=12.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3d01b6e8d59dd85d83fb0a81e344145fea0159fc"
}
