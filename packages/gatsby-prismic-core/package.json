{
  "name": "gatsby-prismic-core",
  "version": "0.0.0",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/gatsby-plugin-core.js",
  "module": "dist/gatsby-plugin-core.modern.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "yarn clean && microbundle watch src/index.ts -f modern,cjs --compress=false",
    "build": "yarn clean && microbundle build src/index.ts -f modern,cjs --compress=false",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "dependencies": {
    "camel-case": "^4.1.1",
    "fp-ts": "^2.8.2",
    "gatsby-plugin-imgix": "^0.8.2",
    "io-ts": "^2.2.10",
    "io-ts-types": "^0.5.10",
    "pascal-case": "^3.1.1",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^3.0.2"
  },
  "devDependencies": {
    "@types/prismic-dom": "^2.1.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.3"
  }
}
