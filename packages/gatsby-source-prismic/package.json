{
  "name": "gatsby-source-prismic",
  "private": true,
  "version": "4.0.0",
  "description": "Gatsby source plugin for building websites using prismic.io as a data source",
  "license": "MIT",
  "main": "./dist/gatsby-source-prismic.js",
  "exports": {
    "./": "./dist/gatsby-source-prismic.modern.js",
    "./gatsby-node": "./dist/gatsby-node.js",
    "./package.json": "./package.json"
  },
  "module": "./dist/gatsby-source-prismic.module.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "gatsby-node.js",
    "fragments.js",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle build src/index.ts src/gatsby-node.ts --tsconfig tsconfig.build.json",
    "dev": "microbundle watch src/index.ts src/gatsby-node.ts",
    "format": "prettier --write '{src,docs}/**/*.{ts,tsx,md}'",
    "test": "jest",
    "prepare": "yarn build",
    "release": "standard-version"
  },
  "homepage": "https://github.com/angeloashmore/gatsby-source-prismic",
  "repository": {
    "type": "git",
    "url": "https://github.com/angeloashmore/gatsby-source-prismic.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "prismic"
  ],
  "dependencies": {
    "fp-ts": "^2.9.3",
    "gatsby-node-helpers": "^1.0.3",
    "gatsby-plugin-imgix": "^0.8.2",
    "gatsby-source-filesystem": "^2.9.0",
    "prismic-dom": "^2.2.4",
    "prismic-javascript": "^3.0.2",
    "tiny-hashes": "^1.0.1"
  },
  "peerDependencies": {
    "gatsby": ">=2.29"
  },
  "devDependencies": {
    "@types/prismic-dom": "^2.1.1",
    "@types/react-dom": "^17.0.0",
    "gatsby-image": "^2.9.0",
    "gatsby-plugin-utils": "^0.7.0"
  }
}
